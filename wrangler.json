{
  "compatibility_date": "2025-04-01",
  "main": "src/index.ts",
  "name": "nfc-card-backend",
  "upload_source_maps": true,
  "vars": {
    "CORS": ".twenty-print.com"
  },
  "env": {
    "dev": {
      "vars": {
        "CORS": "localhost"
      },
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "nfc-card-db",
          "database_id": "253a0970-b1f8-432a-aa02-0ca0f52a6bf3"
        }
      ],
      "r2_buckets": [
        {
          "bucket_name": "nfc-card-app",
          "binding": "BUCKET",
          "remote": true
        }
      ],
      "ratelimits": [
        {
          "name": "RATE_LIMITER",
          "namespace_id": "1001",
          "simple": {
            "limit": 100,
            "period": 60
          }
        },
        {
          "name": "LOGIN_RATE_LIMITER",
          "namespace_id": "1002",
          "simple": {
            "limit": 3,
            "period": 60
          }
        }
      ]
    }
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "nfc-card-db",
      "database_id": "4fa1af64-5f99-48ca-8cc9-cdd1b3fe5fab"
    }
  ],
  "r2_buckets": [
    {
      "bucket_name": "nfc-card-app",
      "binding": "BUCKET",
      "remote": true
    }
  ],
  "ratelimits": [
    {
      "name": "RATE_LIMITER",
      "namespace_id": "1001",
      "simple": {
        "limit": 100,
        "period": 60
      }
    },
    {
      "name": "LOGIN_RATE_LIMITER",
      "namespace_id": "1002",
